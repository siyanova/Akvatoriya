<Window x:Class="Akvatoriya.FindAnOrder"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Akvatoriya"
        mc:Ignorable="d"
        Title="FindAnOrder" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" Orientation="Horizontal">
            <Label Content="Order Archive" Margin="10" FontSize="15"/>
            <TextBox Width="400" Height="30"/>
            <Button Content="Find" Margin="10" Height="30" Width="100"/>
        </StackPanel>
        <Frame Grid.Row="1" Margin="0,149,532,0">
            
        </Frame>
        <Border BorderBrush="Black" BorderThickness="1" Grid.Row="1"/>
    </Grid>
</Window>
